{
    "name": "Jumpin.chat Plus",
    "manifest_version": 2,
    "version": "0.0.1",

    "description": "Upgrade your Jumpin.chat room",
    "icons": {
        "16": "support.png",
        "48": "support.png",
        "128": "support.png"
    },

    "content_scripts": [{
      "matches": ["https://jumpin.chat/*"],
      "css": ["font-awesome.min.css", "main.css"],
      "js": ["content.js"],
      "run_at": "document_end"
    }],

    "browser_action": {
        "default_title": "Upgrade your Jumpin.chat room",
        "default_icon": "support.png"
    }

}
